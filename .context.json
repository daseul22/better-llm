{
  "project_name": "claude-flow",
  "language": "python",
  "framework": "claude-agent-sdk",
  "architecture": "Clean Architecture (4-Layer)",
  "key_files": {
    "entry_points": {
      "web": "src/presentation/web/app.py"
    },
    "domain": {
      "models": "src/domain/models/",
      "agents": "src/domain/agents/base.py",
      "services": "src/domain/services/"
    },
    "application": {
      "ports": "src/application/ports/"
    },
    "infrastructure": {
      "worker": "src/infrastructure/claude/worker_client.py",
      "worker_tools": "src/infrastructure/mcp/worker_tools.py",
      "config": "src/infrastructure/config/",
      "storage": "src/infrastructure/storage/"
    },
    "presentation": {
      "web": "src/presentation/web/",
      "routers": "src/presentation/web/routers/",
      "services": "src/presentation/web/services/",
      "schemas": "src/presentation/web/schemas/"
    },
    "config": "config/agent_config.json",
    "system_config": "config/system_config.json",
    "prompts": "prompts/"
  },
  "coding_style": {
    "docstring_style": "google",
    "type_hints": true,
    "line_length": 100,
    "quote_style": "double",
    "import_style": "absolute"
  },
  "dependencies": [
    "claude-agent-sdk",
    "fastapi",
    "uvicorn",
    "sse-starlette",
    "python-dotenv",
    "jinja2",
    "pyyaml",
    "structlog",
    "pytest",
    "pytest-asyncio"
  ],
  "testing": {
    "framework": "pytest",
    "test_directory": "tests/"
  },
  "description": "워크플로우 기반 AI 개발 자동화 시스템 - Clean Architecture 기반, 전문화된 Worker Agent들을 노드로 연결하여 복잡한 소프트웨어 개발 작업을 자동화"
}